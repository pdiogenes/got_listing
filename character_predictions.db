create table pokemon(
    S_No serial not null,
    actual int not null,
    pred int not null,
    alive decimal not null,
    plod varchar(255) not null,
    name varchar(255) not null,
    title varchar(255),
    male varchar(255) not null,
    culture varchar(255),
    dateOfBirth int,
    DateoFdeath int,
    mother varchar(255),
    father varchar(255),
    heir varchar(255),
    house varchar(255),
    spouse varchar(255),
    book1 int not null,
    book2 int not null,
    book3 int not null,
    book4 int not null,
    book5 int not null,
    isAliveMother int,
    isAliveFather int,
    isAliveHeir int,
    isAliveSpouse int,
    isMarried int not null,
    isNoble int not null,
    age int,
    numDeadRelations int not null,
    boolDeadRelations int not null,
    isPopular int not null,
    popularity decimal not null,
    isAlive int not null,
    primary key(S_No)
);
